---
import OmptimizedPortfolioImages from "@lib/components/OmptimizedPortfolioImages.astro";
import BaseLayout from "@lib/layouts/BaseLayout.astro";
import GridDisplay from "@lib/layouts/GridDisplay.astro";

const images = await Astro.glob<{ default: ImageMetadata }>(
    "@assets/seed/img/*"
);
---

<BaseLayout title="snap seed">
    <GridDisplay>
        <OmptimizedPortfolioImages {images} />
    </GridDisplay>
</BaseLayout>
